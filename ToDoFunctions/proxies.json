{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "API": {
      "matchCondition": {
        "route": "api/{*restOfPath}"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/{restOfPath}"
    },
    "Files": {
      "matchCondition": {
        "route": "{*path}"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/StaticFileServer?file={path}"
    }
  }
}