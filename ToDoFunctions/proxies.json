{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "HomePage": {
      "matchCondition": {
        "route": "/"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/Page/Index"
    },
    "RoutingTest": {
      "matchCondition": {
        "route": "/{page}"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/{page}"
    }
  }
}